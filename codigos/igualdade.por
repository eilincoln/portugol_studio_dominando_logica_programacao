programa
{
	
	funcao inicio()
	{
		// IGUALDADE
		inteiro valor1, valor2, resultado

		escreva("Digite o valor 1: ")
		leia(valor1)
		escreva("Digite o valor 2: ")
		leia(valor2)

		se(valor1 == valor2) {
			escreva("Os valores são iguais.")
		} senao {
			escreva("Os valores são diferentes.")
		}
	}
}
