programa
{
	
	funcao inicio()
	{
		//MENOR OU IGIAL (>=)
		inteiro valor1, valor2, resultado

		escreva("Digite o valor 1: ")
		leia(valor1)
		escreva("Digite o valor 2: ")
		leia(valor2)

		se(valor1 <= valor2) {
			escreva("O valor 1 é MENOR ou IGUAL que o 2.")
		} senao {
			escreva("O valor 2 é MENOR ou IGUAL que o 1.")
		}
	}
}
